{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      {
        "singleQuote": true
      }
    ],
    "no-unused-vars": "warn",
    "prefer-const": "off",
    "no-import-assign": "off",
    "no-unused-expressions": "warn"
  }
}
